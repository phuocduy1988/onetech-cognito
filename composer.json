{
    "name": "onetech/cognito",
    "description": "User manage / login / register / forgot password with aws cognito",
    "keywords": [
        "php",
        "laravel",
        "aws",
        "cognito",
        "auth",
        "authentication",
        "oauth",
        "user pool",
        "Onetech"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Phuoc Duy",
            "email": "phuocduy1988@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "aws/aws-sdk-php": "^3.268"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0"
    },
    "autoload": {
        "psr-4": {
            "Onetech\\Cognito\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "develop": "1.0.0"
        },
        "laravel": {
            "providers": [
                "Onetech\\Cognito\\CognitoServiceProvider"
            ]
        }
    }
}
